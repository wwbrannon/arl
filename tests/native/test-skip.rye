;;; Tests for skip functionality
;;; These are demonstration tests showing how to use skip() - they are
;;; intentionally skipped and can be ignored in test output.

(define test-skip-without-reason (lambda ()
  ;; Demo: skip without reason
  (skip "[DEMO] Example of skip() without reason - ignore this skip")
  ;; This should never be reached
  (assert-equal 1 2)))

(define test-skip-with-reason (lambda ()
  ;; Demo: skip with custom reason
  (skip "[DEMO] Example of skip() with reason - ignore this skip")
  ;; This should never be reached
  (assert-equal 1 2)))

(define test-skip-conditional (lambda ()
  ;; Demo: conditional skip
  (if (> 2 1)
    (skip "[DEMO] Example of conditional skip() - ignore this skip")
    (assert-equal 1 1))))

(define test-normal-passing (lambda ()
  ;; This test should pass normally
  (assert-equal 1 1)))
